{
  "expo": {
    "name": "Land 2 Lavish",
    "slug": "land-2-lavish",
    "version": "1.0.0",
    "owner": "ankitch2866",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "sdkVersion": "54.0.0",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ankitch2866.land2lavish"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ankitch2866.land2lavish",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "2.0.21",
            "jvmTarget": "17"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b9d24cf8-8b70-441c-ab5d-715bccd2f321"
      }
    }
  }
}
